// variables
$red : #fd7878;
$green : #6ee8ca;
$yellow : #f6e993;


// MIXINS
@mixin hideProperly(){
  display: none;
  position: absolute;
  top:-10000px;
}
@mixin width-wrapper($width)
{
  width: $width;
  margin:
  {
    left: auto;
    right: auto;
  }
}

