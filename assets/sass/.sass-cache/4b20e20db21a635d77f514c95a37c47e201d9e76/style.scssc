        /* MENU */
/* -------------------- */

div#wrapper
{
  section#menu
  {
    ul
    {
      /* SUBMENU */

      li
      {
        ul#submenu
        {
          /*display: none;*/
          position: absolute;
          width: 150px;
          left: -60px;
          top: 50px;
          opacity: 0;
          @include single-transition(opacity,1s);
          @include border-radius(5px);
          background-color: #fbf9f4;
          @include box-shadow(rgba(0,0,0,.2) 0px 5px 5px);

          > li
          {
            display: block;
            border: none;
            width: 100px;
            padding: 15px;
            font-size: 18px;
            border-bottom: 1px solid rgba(0,0,0,0.2);
          }

          >li:last-child
          {
            border: none;
          }

        }

        //
        position: relative;
      
      }

      li:hover
      {

        ul#submenu
        {
          /*display: block;*/

          opacity: 1;
          @include single-transition(opacity,1s);
          @include border-radius(5px);
        }
      }

      /* /SUBMENU */

      li:first-child {border-top: 3px solid $red;}
      li:first-child :hover,li:first-child.active
      {
        color: $red;
      }
      li:last-child {border-top: 3px solid $green;}
      li:last-child :hover,li:last-child.active
      {
        color: $green;
      }
      li{border-top: 3px solid $yellow;}
      li :hover,li.active
      {
        color: $yellow;
      }

      li
      {
        a{
          color: #5f5f5f;
          text-decoration: none;
        }

        //li
        padding-top: 16px;
        margin-left: 20px;
        display: inline-block;
        font-family: "bebasneue";
        font-size: 30px;

      }

      li :hover{
        color: $yellow;
        transition-property:color;
        transition-duration:1s;
        transition-timing-function:linear;

        -webkit-transition-property:color;
        -webkit-transition-duration:1s;
        -webkit-transition-timing-function:linear;
      }

      li:first-child :hover{
        color: $red;
        transition-property:color;
        transition-duration:1s;
        transition-timing-function:linear;

        -webkit-transition-property:color;
        -webkit-transition-duration:1s;
        -webkit-transition-timing-function:linear;
      }

      li:last-child :hover{
        color: $green;
        transition-property:color;
        transition-duration:1s;
        transition-timing-function:linear;

        -webkit-transition-property:color;
        -webkit-transition-duration:1s;
